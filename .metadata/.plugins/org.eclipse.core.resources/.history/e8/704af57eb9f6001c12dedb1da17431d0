package abc257;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class B_02 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int K = sc.nextInt();
		int Q = sc.nextInt();
		//駒の位置
		List<Integer> koma  =new ArrayList<Integer>();
		for(int i=0; i<K; i++) {
			koma.add(sc.nextInt())
		}
		//命令の対象位置
		int[] order = new int[Q];
		for(int i=0; i<Q; i++) {
			int num = sc.nextInt();
			order[i] = num - 1;
			
			System.out.println("koma[order[i]] = "+koma[order[i]]);
			if(koma[order[i]] != N &&  ) {
				koma[order[i]]++;
				System.out.println(koma[order[i]]);
			}
		}
		
//		for(int i=0; i<K; i++) {
//			System.out.println(koma[i]);
//		}
		
	}

}
