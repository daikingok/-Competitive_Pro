package abc235;

import java.util.Scanner;

public class B {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int[] H = new int[N];
		for(int i=0; i<N; i++) {
			H[i] = sc.nextInt();
			if(N != i &&  H[i] < H[i+1]) {
				System.out.println(H[i]);
				break;
			}
		}
		

	}

}
