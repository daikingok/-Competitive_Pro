package abc237;

import java.util.Scanner;

public class A {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Scanner sc = new Scanner(System.in);
		long n = sc.nextInt();
		long right = n*n;
		
		long left =1;
		for(long i=0; i<n; i++) {
			left *= 2;
		}
		System.out.println(left+"   "+right);
		if(left > right) {
			System.out.println("Yes");
		}else {
			System.out.println("No");
		}
	}

}
