package abc234;

public class B {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

	}
	private static double dist( double x1, double y1, double x2, double y2 ) {
		double l;
		double dx,dy;
		dx = x2 - x1;
		dy = y2 - y1;
		l = Math.sqrt( dx * dx + dy * dy );
		return l;
	}

		
	}

}
