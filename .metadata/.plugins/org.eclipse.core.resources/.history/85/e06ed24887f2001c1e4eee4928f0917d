package abc236;

import java.util.Scanner;

public class chokudai {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Scanner sc = new Scanner(System.in);
		String S = sc.next();
		int a = sc.nextInt();
		int b = sc.nextInt();
		char[] str = S.toCharArray();
		
		char change = str[a];
		str[a] = str[b];
		str[b] = change;
		String ans = str.toString();
		System.out.println(ans);
	}

}
