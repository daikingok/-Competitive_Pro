package abc242;

import java.util.Scanner;

public class A {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int A = sc.nextInt();
		int B = sc.nextInt();
		int C = sc.nextInt();
		int X = sc.nextInt();
		if(A  <= X) {
			System.out.println(1);
		}else if(A > X) {
			double  sum = (B-A)/C;
		}
	}
}
