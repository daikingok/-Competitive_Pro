package abc220;

import java.util.Scanner;

public class C {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Scanner sc = new Scanner(System.in);
		Long N = sc.nextLong();
		Long[] array = new Long[N];
		for(int i=0; i<N; i++) {
			array[i] = sc.nextLong();
		}
		Long X = sc.nextLong();
		
		Long ans = 0L;
		Long index = 0L;
		Long count = 0L;
		while(ans <= X) {
			if(index == N) {
				index = 0L;
			}
			ans += array[index];
			index++;
			count++;
			
		}
		System.out.println(count);
	}

}
