package paiza;

import java.util.Scanner;

public class C087 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int reverse =  0;
		
		while(reverse = N) {
			for(int i = N; 0 < N; N /= 10) {
				System.out.print(N % 10);
			}
		}
		
		
	}

}
