package atCoder_C;

import java.util.Scanner;

public class C062 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		String[] array = new String[T]; 

		for(int i=0; i<15; i++) {
			if(array[i].contains("melon")) {
				System.out.println(i);
			}
		}
	}

}
