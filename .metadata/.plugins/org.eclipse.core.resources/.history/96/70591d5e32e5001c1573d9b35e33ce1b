package paiza;

import java.util.Scanner;

public class C095 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Scanner sc = new Scanner(System.in);
		int N =sc.nextInt();
		int K = sc.nextInt();
		int[] time = new int[N];
		for(int i=0; i<N; i++) {
			int num = sc.nextInt();
			time[i] = Math.abs(K-num)
		}
	}

}
