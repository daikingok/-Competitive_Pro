package 典型90;

import java.util.Scanner;

public class a04 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Scanner sc = new Scanner(System.in);
		int H = sc.nextInt();
		int W = sc.nextInt();
		int[][] arr = new int[H][W];
		int[] hs = new int[H];
		int[] ws = new int[W];
		for(int i=0; i<H; i++) {
			for(int j=0; j<W; j++) {
				int num = sc.nextInt();
				arr[i][j] = num;
				hs[i] += num;
				ws[i] +=num;
			}
		}
		for(int i=0; i<H; i++) {
			for(int j=0; j<W; j++) {
				
				System.out.print(hs[i] + ws[i] - arr[i][j]);
				System.out.print(" ");
			}
			System.out.println();
		}
		
	}

}
