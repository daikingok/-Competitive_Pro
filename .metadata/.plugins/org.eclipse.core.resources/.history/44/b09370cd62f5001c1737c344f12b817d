package abc257;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

public class B {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int K = sc.nextInt();
		int Q = sc.nextInt();
	
		//駒動かす対象の取得
		List<Integer> list = new ArrayList<Integer>();
		for(int i=0; i<K; i++) {
			list.add(sc.nextInt());
		}
		
		int[] L = new int[Q];
	   //駒の位置取得
		for(int i=0; i<Q; i++) {
			int place = sc.nextInt();
			L[i]  = place -1;
		}
		
		List<Integer> koma = new ArrayList<Integer>();
		//駒あれば1なければ2
		for(int i =0; i<N; i++) {
			if(list.contains(i)) {
				koma.add(1);
			}else {
				koma.add(0);
			}
			System.out.println(koma.get(i));
		}
			
		
	}

}
