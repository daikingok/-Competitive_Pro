package abc258;

import java.util.Scanner;

public class C {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Scanner sc = new Scanner(System.in);
		int N = sc.nextInt();
		int Q = sc.nextInt();
		String S = sc.next();
		for(int i = 0; i<Q; i++) {
			int query = sc.nextInt();
			int taisyou = sc.nextInt();

			if(query == 1) {
				for(int j=0; j<taisyou; j++) {
					s = str_to_rst(S)
				}

			}else if(query == 2) {
				System.out.println(S.charAt(taisyou-1));
			}
		}
	}
	private static String str_to_rst(String s) {
		String[] str = s.split("");
		//対象文字
		String ss = str[s.length()-1];
		str[s.length()-1] = "";
		String ans = String.join("", str);
		s = ss + ans;
		return s;
	}

}
