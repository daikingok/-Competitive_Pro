package atCoder_B;

import java.util.Scanner;

public class B_KthCommonDivisor {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Scanner sc = new Scanner(System.in);
		int A = sc.nextInt();
		int B = sc.nextInt();
		int K = sc.nextInt();
		
		int[] ans = new int[100];
	    int c = 0;
	    int s;
	    if(A >= B) {
	    	s = B;
	    }else {
	    	s = A;
	    }
		for(int i=0; i<s; i++) {
			if(A % i ==0 && B % i == 0) {
				ans[c] = i;
				c++;
			}
		}
		System.out.println(ans.length);
	}

}
